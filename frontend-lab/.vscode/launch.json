  "version": "0.2.0",
  "configurations": [
    {
      "name": "ng serve",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "npm: start",
      "url": "http://LOCALHOST:4200/",
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
      "webpack:/*": "${webRoot}/*"
      }
    },
    {
      "name": "ng test",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "npm: test",
      "url": "http://LOCALHOST:9876/debug.html"
    }
  ]
}
